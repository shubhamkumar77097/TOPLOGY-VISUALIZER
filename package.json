{
  "name": "toplogy-visualizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:with-mock": "node ./scripts/mock-server.js & sleep 0.2; next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "mock-server": "node ./scripts/mock-server.js",
    "dev:wasm": "USE_WASM_SQLITE=1 NODE_OPTIONS=--max_old_space_size=4096 next dev --webpack",
    "prune": "node ./scripts/prune-worker.js",
    "prune:once": "RUN_ONCE=1 node ./scripts/prune-worker.js",
    "seed-history": "node ./scripts/seed-history.js",
    "import-data": "node ./scripts/import-data.js",
    "prepare-release": "node ./scripts/refresh-history-db.js",
    "smoke-test": "node ./scripts/history-smoke-test.js",
    "worker:run": "node ./scripts/worker-direct.js",
    "worker:schedule": "node ./scripts/schedule-probes.js",
    "pm2:start-worker": "pm2 start ./scripts/worker-direct.js --name topology-worker --cron \"*/1 * * * *\"",
    "compute-rollups": "node ./scripts/compute-rollups.js",
    "pm2:start-rollups": "pm2 start ./scripts/compute-rollups.js --name topology-rollups --cron \"0 * * * *\"",
    "lint": "eslint",
    "test": "jest --runInBand",
    "test:unit": "jest --runInBand",
    "test:e2e": "npx playwright test",
    "test:ci": "npm run lint && npm run build && npm run test:unit && npx playwright install --with-deps && npx playwright test --forbid-only"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 20000,
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "chart.js": "^4.5.1",
    "earcut": "^2.2.2",
    "framer-motion": "^12.23.24",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.0",
    "sql.js": "^1.13.0",
    "three": "^0.181.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19",
    "@types/three": "^0.181.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^29.6.1",
    "minimist": "^1.2.8",
    "node-fetch": "^2.7.0",
    "playwright": "^1.40.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "ws": "^8.18.3"
  }
}
